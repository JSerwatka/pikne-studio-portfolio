{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@components/*": ["src/components/*"],
            "@layouts/*": ["src/layouts/*"],
            "@assets/*": ["src/assets/*"],
            "@utils/*": ["src/utils/*"],
            "@store/*": ["src/store/*"]
        },
        "jsx": "preserve",
        "jsxImportSource": "solid-js"
    }
}
