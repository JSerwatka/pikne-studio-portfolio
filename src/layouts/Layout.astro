---
import Navbar from "@components/Navbar/Navbar.astro";
import LayoutBase from "./LayoutBase.astro";
import Footer from "@components/Footer.astro";

interface Props {
    title: string;
}

const { title } = Astro.props;
---

<LayoutBase title={title}>
    <Navbar />
    <main class="grow">
        <slot />
    </main>
    <Footer />
</LayoutBase>
